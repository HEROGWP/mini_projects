<h1>List</h1>
<table>
	<tr>
		<th>Name</th>
		<th>URL</th>
		<th>Description</th>
		<th></th>
	</tr>
	<% @videos.each do |v| %>
		
		<tr>
			<td><%= v.name %></td>
			<td><%= v.url_address %></td>
			<td><%= v.description %></td>
			<td>
				<%= link_to "edit", edit_video_path(v)%>
				<%= link_to "destroy", video_path(v), :methods => :delete %>
			</td>
		</tr>
	
	<% end %>
</table>